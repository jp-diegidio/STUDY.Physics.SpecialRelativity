		"indentalign",
		"indentshift",
		"infty",
		"irng",
		"mathbb",
		"mathjax",
		"mathrm",
		"mathstrut",
		"merror",
		"mtext",
		"oper",
		"operatorname",
		"simul",
		"simuls",
		"subseteq"
	],
	"cSpell.words": [
		"chtml",
